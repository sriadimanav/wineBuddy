{
  "name": "wine_buddy",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sriadimanav/wineBuddy.git"
  },
  "homepage": "https://github.com/sriadimanav/wineBuddy#readme",
  "author": "Aditya <aditya.srivastava881@gmail.com>",
  "license": "MIT",
  "description": "Wine information finder",
  "keywords": [
    "wine",
    "buddy",
    "react",
    "vite"
  ],
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "start": "vite --port 3000",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "format": "prettier --config prettier.config.js --write .",
    "format:check": "prettier --config prettier.config.js --check .",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "validate": "pnpm run type-check && pnpm run lint && pnpm run format:check && pnpm run test",
    "clean": "rm -rf node_modules dist dev-dist .turbo coverage pnpm-lock.yaml",
    "clean:cache": "rm -rf node_modules/.cache .vite dist dev-dist",
    "preview": "vite preview --port 3001",
    "analyze": "pnpx vite-bundle-analyzer --port 8889",
    "analyze:auto-port": "pnpx vite-bundle-analyzer --port 0",
    "analyze:json": "vite build --mode analyze && pnpx vite-bundle-analyzer dist --mode json",
    "analyze:vite": "vite build --mode analyze"
  },
  "dependencies": {
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slot": "1.2.3",
    "@tanstack/react-query": "5.85.0",
    "@tanstack/react-router": "1.131.7",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.539.0",
    "radix-ui": "1.4.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-error-boundary": "6.0.0",
    "react-hook-form": "7.62.0",
    "tailwind-merge": "3.3.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "4.1.11",
    "@tanstack/eslint-config": "0.3.0",
    "@tanstack/eslint-plugin-query": "5.83.1",
    "@tanstack/react-query-devtools": "5.85.0",
    "@tanstack/react-router-devtools": "1.131.7",
    "@tanstack/router-plugin": "1.131.7",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.0",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "24.2.1",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react": "5.0.0",
    "jsdom": "26.1.0",
    "prettier": "3.6.2",
    "serve": "14.2.4",
    "tailwindcss": "4.1.11",
    "typescript": "5.9.2",
    "vite": "7.1.2",
    "vite-bundle-analyzer": "1.2.1",
    "vite-plugin-pwa": "1.0.2",
    "vitest": "3.2.4",
    "web-vitals": "5.1.0",
    "workbox-window": "7.3.0"
  },
  "engines": {
    "node": ">=22.18.0 <23",
    "pnpm": ">=10.14.0 <11"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ]
}
